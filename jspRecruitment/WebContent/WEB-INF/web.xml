<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>jspRecruitment</display-name>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>jsprecruitment.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>CompanyRegServlet</servlet-name>
    <servlet-class>jsprecruitment.servlet.company.CompanyRegServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CompanyRegServlet</servlet-name>
    <url-pattern>/CompanyRegServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>UpdateCompanyInfoServlet</servlet-name>
    <servlet-class>jsprecruitment.servlet.company.UpdateCompanyInfoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UpdateCompanyInfoServlet</servlet-name>
    <url-pattern>/UpdateCompanyInfoServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ComapnyChangePasswordServlet</servlet-name>
    <servlet-class>jsprecruitment.servlet.company.ComapnyChangePasswordServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ComapnyChangePasswordServlet</servlet-name>
    <url-pattern>/ComapnyChangePasswordServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>